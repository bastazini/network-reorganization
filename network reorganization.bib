@article{ WOS:000292639100002,
Author = {Moellmann, Christian and Conversi, Alessandra and Edwards, Martin},
Title = {Comparative analysis of European wide marine ecosystem shifts: a
   large-scale approach for developing the basis for ecosystem-based
   management},
Journal = {BIOLOGY LETTERS},
Year = {2011},
Volume = {7},
Number = {4},
Pages = {484-486},
Month = {AUG 23},
Abstract = {Abrupt and rapid ecosystem shifts (where major reorganizations of
   food-web and community structures occur), commonly termed regime shifts,
   are changes between contrasting and persisting states of ecosystem
   structure and function. These shifts have been increasingly reported for
   exploited marine ecosystems around the world from the North Pacific to
   the North Atlantic. Understanding the drivers and mechanisms leading to
   marine ecosystem shifts is crucial in developing adaptive management
   strategies to achieve sustainable exploitation of marine ecosystems. An
   international workshop on a comparative approach to analysing these
   marine ecosystem shifts was held at Hamburg University, Institute for
   Hydrobiology and Fisheries Science, Germany on 1-3 November 2010.
   Twenty-seven scientists from 14 countries attended the meeting,
   representing specialists from seven marine regions, including the Baltic
   Sea, the North Sea, the Barents Sea, the Black Sea, the Mediterranean
   Sea, the Bay of Biscay and the Scotian Shelf off the Canadian East
   coast. The goal of the workshop was to conduct the first large-scale
   comparison of marine ecosystem regime shifts across multiple regional
   areas, in order to support the development of ecosystem-based management
   strategies.},
